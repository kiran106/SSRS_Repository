      </Fields>
      <Query>
        <DataSourceName>REGION_PERSON_DS</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT        PERSON.ID AS Expr1, PERSON.FIRST_NAME, PERSON.LAST_NAME, REGION.REGION, REGION.REG_CODE
FROM            REGION RIGHT OUTER JOIN
                         PERSON ON REGION.REG_ID = PERSON.REG_ID
ORDER BY PERSON.FIRST_NAME DESC, Expr1</CommandText>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
